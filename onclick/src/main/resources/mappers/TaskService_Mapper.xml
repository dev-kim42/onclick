<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.onclick.app.persistence.TaskService_Mapper">

<select id="taskSelectOne" parameterType="Integer" resultType="tv">
SELECT *
FROM onclick_taskupload
WHERE tudelyn='N'
AND tuidx=#{tuidx}
</select>

<select id="taskSelectAll" parameterType="Integer" resultType="tv">
SELECT *
FROM onclick_taskupload
WHERE tudelyn='N'
AND lidx=#{lidx}
</select>

<insert id="taskInsert" parameterType="HashMap">
INSERT INTO onclick_taskupload (TUIDX,TUNAME,TUSTART,TUFIN,TUDATE,TUING,TUFILE,TUNOTYN,LIDX,TUCONTENTS)
VALUES (tuidx_seq.nextval,#{tusubject},#{tustart},#{tufin},sysdate,'진행',#{tufile},'Y',#{lidx},#{tucontents})
</insert>



</mapper> 